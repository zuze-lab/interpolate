{
  "name": "@zuze/interpolate",
  "version": "7.0.0",
  "description": "A tiny library for performing simple (and complex) interpolations.",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "browser": "build/bundle.min.js",
  "sideEffects": false,
  "keywords": [
    "interpolate",
    "interpolation",
    "map",
    "object mapping",
    "transforms",
    "transformations",
    "flat"
  ],
  "files": [
    "build"
  ],
  "homepage": "https://github.com/zuze-lab/interpolate",
  "scripts": {
